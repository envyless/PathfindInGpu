#ifndef CommonCS
#define CommonCS
#define ArrayStruct(StructName ,T, Num) struct StructName { T buffer[Num]; int index; int MaxCount; \
        void Add(T input)\
        { buffer[index] = input; index++;}\
        \
        T Last()\
        { return buffer[index];\
        } \
        void Init(){ MaxCount = Num; index = 0;}};\
        
        
#endif